macro canMoveFriend = {
    if (= ahead friend) then {
        if (random 2) then {
            turn left;
            if (= ahead friend) then {
                turn left;
                move
            }
            else {
                move
            }
        }
        else {
            turn right;
            if (= ahead friend) then {
                turn right;
                move
            }
            else {
                move
            }
        }
    }
    else {
        move
    }
};

macro canMove = {
    if (= ahead rock) then {
        if (random 2) then {
            turn left;
            turn left;
            if (= ahead rock) then {
                turn left;
                call canMoveFriend
            }
            else {
                call canMoveFriend
            }
        }
        else {
            turn right;
            turn right;
            if (= ahead rock) then {
                turn right;
                call canMoveFriend
            }
            else {
                call canMoveFriend
            }
        }
    }
    else {
        call canMoveFriend
    }
};

macro moveTest = {
